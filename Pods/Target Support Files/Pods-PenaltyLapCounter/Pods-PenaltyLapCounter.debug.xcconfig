CLANG_CXX_LANGUAGE_STANDARD = compiler-default
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/Realm.framework/Headers"
OTHER_CPLUSPLUSFLAGS = -std=c++1y $(inherited)
OTHER_LDFLAGS = $(inherited) -ObjC -framework "Realm"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-PenaltyLapCounter
PODS_ROOT = ${SRCROOT}/Pods